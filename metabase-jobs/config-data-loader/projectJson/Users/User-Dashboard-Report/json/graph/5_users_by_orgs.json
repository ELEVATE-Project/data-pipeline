        "query": "SELECT\r\n  meta.attribute_label AS organization_id,\r\n  meta.attribute_value AS organization_name,\r\n  COUNT(DISTINCT meta.user_id) AS user_count\r\nFROM\r\n  ${tenantUserMetadataTable} meta\r\nJOIN\r\n  ${tenantUserTable} u ON u.user_id = meta.user_id\r\nWHERE\r\n  u.status = 'ACTIVE'\r\nAND  \r\n  attribute_code = 'Organizations'\r\n\r\n  [[AND u.user_profile_one_name = (\r\n    SELECT u.user_profile_one_name\r\n    FROM ${tenantUserTable}\r\n    WHERE {{state_param}}\r\n    LIMIT 1)]]\r\n\r\n  [[AND u.user_profile_two_name = (\r\n    SELECT u.user_profile_two_name\r\n    FROM ${tenantUserTable}\r\n    WHERE {{district_param}}\r\n    LIMIT 1)]]\r\n    \r\n  [[AND u.user_profile_three_name = (\r\n    SELECT u.user_profile_three_name\r\n    FROM ${tenantUserTable}\r\n    WHERE {{block_param}}\r\n    LIMIT 1)]]\r\n    \r\n  [[AND u.user_profile_four_name = (\r\n    SELECT u.user_profile_four_name\r\n    FROM ${tenantUserTable}\r\n    WHERE {{cluster_param}}\r\n    LIMIT 1)]]\r\n    \r\n  [[AND meta.user_id IN (\r\n    SELECT meta.user_id\r\n    FROM ${tenantUserMetadataTable}\r\n    WHERE attribute_code = 'Organizations'\r\n      AND {{organizations_param}})]]\r\n\r\n  [[AND meta.user_id IN (\r\n    SELECT meta.user_id\r\n    FROM ${tenantUserMetadataTable}\r\n    WHERE attribute_code = 'Platform Role'\r\n      AND {{platform_role_param}})]]\r\n      \r\n  [[AND meta.user_id IN (\r\n    SELECT meta.user_id\r\n    FROM ${tenantUserMetadataTable}\r\n    WHERE attribute_code = 'Professional Role'\r\n      AND {{professional_role_param}})]]\r\n      \r\n  [[AND meta.user_id IN (\r\n    SELECT meta.user_id\r\n    FROM ${tenantUserMetadataTable}\r\n    WHERE attribute_code = 'Professional Subroles'\r\n      AND {{professional_subroles_param}})]]\r\n      \r\nGROUP BY\r\n  meta.attribute_label, meta.attribute_value\r\nORDER BY\r\n  user_count DESC;\r\n"
      }
    },
    "display": "pie",
    "visualization_settings": {
