{
  "questionCard": {
    "name": "In Progress",
    "collection_id": 644,
    "dataset_query": {
      "database": 2,
      "type": "native",
      "native": {
        "query": "SELECT\n  COUNT(DISTINCT project_id) AS \"no_of_projects_inprogress\"\nFROM\n  ${config.projects}\nWHERE 1 = 1\nAND status = 'inProgress'\n  [[AND ${config.projects}.district_id = (\n        SELECT district_id\n        FROM ${config.projects}\n        WHERE {{district_param}} \n        AND ${config.projects}.state_id = ${state_id}\n        LIMIT 1)\n  ]] \n  [[AND ${config.projects}.program_id = (\n        SELECT ${config.projects}.program_id\n        FROM ${config.projects}\n        JOIN ${config.solutions} ON ${config.projects}.solution_id = ${config.solutions}.solution_id\n        WHERE {{program_param}} \n        AND ${config.projects}.district_id = ${district_id}\n        AND ${config.projects}.state_id = ${state_id}\n        LIMIT 1 )\n   ]]\n[[AND ${config.projects}.block_id = (\n        SELECT block_id\n        FROM ${config.projects}\n        WHERE {{block_param}} \n        AND ${config.projects}.district_id = ${district_id}\n        AND ${config.projects}.state_id = ${state_id}\n        LIMIT 1\n        )\n   ]] \n   [[AND ${config.projects}.cluster_id = (\n        SELECT cluster_id\n        FROM ${config.projects}\n        WHERE {{cluster_param}} \n        AND ${config.projects}.district_id = ${district_id}\n        AND ${config.projects}.state_id = ${state_id}\n        LIMIT 1\n        )\n   ]]\n   [[AND ${config.projects}.org_id = (\n        SELECT org_id\n        FROM ${config.projects}\n        WHERE {{org_param}} \n        AND ${config.projects}.district_id = ${district_id}\n        AND ${config.projects}.state_id = ${state_id}\n        LIMIT 1\n        )\n   ]]",
