        "query": "WITH\n  split_categories AS (\n    SELECT\n      solution_id,\n      UNNEST(STRING_TO_ARRAY(categories, ',')) AS category\n    FROM\n      ${config.solutions}\n  ),\n  cleaned_categories AS (\n    SELECT\n      solution_id,\n      INITCAP(TRIM(category)) AS category\n    FROM\n      split_categories\n  ),\n  yearly_projects AS (\n    SELECT\n      EXTRACT(\n        YEAR\n        FROM\n          created_date::timestamp\n      ) AS project_year,\n      COUNT(*)::INTEGER AS project_count\n    FROM\n      ${config.projects}\n    WHERE\n      1 = 1 [[AND ${config.projects}.state_id = (\n        SELECT state_id\n        FROM ${config.projects}\n        WHERE {{state_name}}\n        LIMIT 1)\n  ]] [[AND ${config.projects}.district_id = (\n        SELECT district_id\n        FROM ${config.projects}\n        WHERE {{district_name}}\n        LIMIT 1)\n  ]]\n    GROUP BY\n      project_year\n  )\nSELECT\n  project_year AS \"Year\",\n  project_count AS \"No of. IMPs Triggered\"\nFROM\n  yearly_projects\nORDER BY\n  project_count DESC\nLIMIT\n  5;"
      }
    },
