      "type": "native",
      "native": {
        "template-tags": {},
        "query": "WITH split_categories AS (\n    SELECT\n        solution_id,\n        UNNEST(STRING_TO_ARRAY(categories, ',')) AS category\n    FROM\n         ${config.solutions}\n),\ncleaned_categories AS (\n    SELECT\n        solution_id,\n        INITCAP(TRIM(category)) AS category\n    FROM\n        split_categories\n)\nSELECT\n    cc.category,\n    COUNT(p.project_id) AS project_count\nFROM\n    cleaned_categories cc\nLEFT JOIN\n     ${config.projects} p\nON\n    cc.solution_id = p.solution_id\nWHERE \n    p.district_id = '66bf7eb960de1616f42cb984'\nGROUP BY\n    cc.category\nORDER BY\n    project_count DESC;\n"
      }
    },
