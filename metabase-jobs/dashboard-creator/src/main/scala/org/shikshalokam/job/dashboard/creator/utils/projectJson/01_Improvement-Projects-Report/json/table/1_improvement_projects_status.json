        },
        "query": "SELECT \n    solutions.programname AS \"Program Name\",\n    solutions.name AS \"Improvement Project Name\",\n    projects.statename,\n    projects.districtname AS \"District Name\",\n    projects.orgname AS \"Organisation Name\",\n    SUM(CASE WHEN projects.status = 'started' THEN 1 ELSE 0 END) AS \"Started\",\n    SUM(CASE WHEN projects.status = 'inProgress' THEN 1 ELSE 0 END) AS \"In-Progress\",\n    SUM(CASE WHEN projects.status = 'inProgress' AND projects.evidence IS NOT NULL THEN 1 ELSE 0 END) AS \"In progress projects with evidence\",\n    SUM(CASE WHEN projects.status = 'submitted' THEN 1 ELSE 0 END) AS \"Submitted\"\nFROM projects \nJOIN solutions  ON projects.solutionid = solutions.solutionid\nWHERE projects.statename is not null \n[[AND {{state_param}}]]\n[[AND {{program_param}}]]\n[[AND {{district_param}}]]\nGROUP BY solutions.programname, solutions.name,projects.statename, projects.districtname, projects.orgname\nORDER BY \"Submitted\" DESC; "
      }
    },
