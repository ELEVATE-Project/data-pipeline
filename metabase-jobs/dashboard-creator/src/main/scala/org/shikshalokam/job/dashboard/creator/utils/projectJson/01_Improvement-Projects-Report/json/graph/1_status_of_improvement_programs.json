        "query" : "SELECT DISTINCT(solutions.programname) AS \"Programs\", \nCOUNT(DISTINCT CASE WHEN projects.status = 'started' THEN projects.projectid END) AS \"Started Projects\", \nCOUNT(DISTINCT CASE WHEN projects.status = 'inProgress' THEN projects.projectid END) AS \"In Progress Projects\", \nCOUNT(DISTINCT CASE WHEN projects.status = 'submitted' THEN projects.projectid END) AS \"Submitted Projects\", \nCOUNT(DISTINCT projects.projectid) AS \"Total Projects\" \nFROM projects \njoin solutions on projects.solutionid = solutions.solutionid\nWHERE solutions.programname is not null\n [[AND {{state_param}}]] \n[[AND {{program_param}}]]\n[[AND {{district_param}}]]\nGROUP BY solutions.programname\nORDER BY \"Total Projects\" DESC;"
      }
