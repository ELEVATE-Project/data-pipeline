{
  "questionCard": {
    "name": "Status of improvement projects",
    "collection_id": 297,
    "dataset_query": {
      "database": 2,
      "type": "native",
      "native": {
        "template-tags": {
          "state_param": {
            "type": "dimension",
            "name": "state_param",
            "id": "d1a52499-2ae6-4417-8f1e-a54781cc8865",
            "display-name": "State Param",
            "default": null,
            "dimension": [
              "field",
              80,
              null
            ],
            "widget-type": "string/=",
            "options": null
          }
        },
        "query": "SELECT DISTINCT(statename) AS \"State\", \nCOUNT(DISTINCT CASE WHEN status = 'started' THEN projectid END) AS \"Started Projects\", \nCOUNT(DISTINCT CASE WHEN status = 'inProgress' THEN projectid END) AS \"In Progress Projects\", \nCOUNT(DISTINCT CASE WHEN status = 'submitted' THEN projectid END) AS \"Submitted Projects\", \nCOUNT(DISTINCT projectid) AS \"Total Projects\" \nFROM projects \nWHERE statename is not null \n[[AND {{state_param}}]] \nGROUP BY statename \nORDER BY \"Total Projects\" DESC;"
      }
    },
    "display": "bar",
