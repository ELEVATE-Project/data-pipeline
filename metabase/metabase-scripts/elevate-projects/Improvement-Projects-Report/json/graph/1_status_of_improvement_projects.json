{
  "questionCard": {
    "name": "Status of improvement projects",
    "collection_id": 86,
    "dataset_query": {
      "database": 2,
      "type": "native",
      "native": {
        "query": "SELECT DISTINCT(statename) AS \"State\", COUNT(DISTINCT CASE WHEN status = 'started' THEN projectid END) AS \"Started Projects\", COUNT(DISTINCT CASE WHEN status = 'inProgress' THEN projectid END) AS \"In Progress Projects\", COUNT(DISTINCT CASE WHEN status = 'submitted' THEN projectid END) AS \"Submitted Projects\", COUNT(DISTINCT projectid) AS \"Total Projects\" FROM projects WHERE 1=1 [[AND statename = {{state_param}}]] GROUP BY statename ORDER BY \"Total Projects\" DESC;",
        "template-tags": {
          "state_param": {
            "type": "text",
            "name": "state_param",
            "display-name": "State Param"
          }
        }
      }
    },
    "display": "bar",
    "visualization_settings": {
      "stackable.stack_type": "stacked",
      "graph.dimensions": [
        "State"
      ],
      "series_settings": {
        "In Progress Projects": {
          "color": "#509EE3"
        },
        "Started": {
          "color": "#F7C4C4"
        },
        "Submitted": {
          "color": "#A7D07C"
        },
        "In Progress": {
          "color": "#FBE499"
        },
        "Submitted Projects": {
          "color": "#F2A86F"
        },
        "Started Projects": {
          "color": "#88BF4D"
        }
      },
      "graph.label_value_formatting": "auto",
      "graph.y_axis.axis_enabled": true,
      "graph.metrics": [
        "Submitted Projects",
        "In Progress Projects",
        "Started Projects"
      ],
      "graph.y_axis.scale": "linear",
      "graph.x_axis.axis_enabled": true,
      "graph.show_values": false,
      "graph.y_axis.title_text": "No. of Improvements",
      "graph.show_goal": false
    }
  },
