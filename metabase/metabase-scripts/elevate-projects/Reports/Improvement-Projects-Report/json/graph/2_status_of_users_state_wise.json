{
  "questionCard": {
    "name": "Status of users state wise",
    "collection_id": 111,
    "dataset_query": {
      "database": 2,
      "type": "native",
      "native": {
        "template-tags": {
          "state_param": {
            "type": "dimension",
            "name": "state_param",
            "id": "f83c21f9-6a17-4c7d-b517-d89a4ef9e502",
            "display-name": "State Param",
            "default": null,
            "dimension": [
              "field",
              80,
              null
            ],
            "widget-type": "string/=",
            "options": null
          }
        },
        "query": "WITH UserProjectCounts AS (\n    SELECT\n        statename,\n        createdby,\n        COUNT(DISTINCT CASE WHEN status = 'started' THEN projectid END) AS \"Started Projects\",\n        COUNT(DISTINCT CASE WHEN status = 'inProgress' THEN projectid END) AS \"In Progress Projects\",\n        COUNT(DISTINCT CASE WHEN status = 'submitted' THEN projectid END) AS \"Submitted Projects\"\n    FROM projects\n    WHERE \n        statename is not null \n        [[ AND {{state_param}} ]]\n    GROUP BY statename, createdby\n)\nSELECT \n    statename AS \"State\",\n    COUNT(DISTINCT CASE WHEN \"Started Projects\" > 0 THEN createdby END) AS \"Started Users\",\n    COUNT(DISTINCT CASE WHEN \"In Progress Projects\" > 0 THEN createdby END) AS \"In Progress Users\",\n    COUNT(DISTINCT CASE WHEN \"Submitted Projects\" > 0 THEN createdby END) AS \"Submitted Users\"\nFROM UserProjectCounts\nGROUP BY statename\nORDER BY \"Submitted Users\" DESC; "
      }
    },
    "display": "bar",
    "visualization_settings": {
      "graph.show_goal": false,
      "graph.y_axis.title_text": "No. of users",
      "graph.show_values": false,
      "graph.x_axis.axis_enabled": true,
      "graph.y_axis.scale": "linear",
      "graph.metrics": [
        "Submitted Users",
        "In Progress Users",
